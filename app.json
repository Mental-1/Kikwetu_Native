{
  "expo": {
    "name": "Kikwetu",
    "slug": "Kikwetu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kikwetu",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kawa-ida.Kikwetu",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kawa_ida.Kikwetu"
    },
    "extra": {
      "eas": {
        "projectId": "7f836722-0d8e-48d8-8c53-2ebad4137d71"
      }
    }
  },
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "cover",
        "backgroundColor": "#1461BD",
        "edgeToEdgeEnabled": true
      }
    ],
    "expo-quick-actions",
    "expo-secure-store",
    "expo-font",
    [
      "@sentry/react-native/expo",
      {
        "url": "https://sentry.io/",
        "project": "kikwetu-native",
        "organization": "routteme"
      }
    ],
    "expo-video",
    "expo-audio",
    "expo-web-browser"
  ],
  "experiments": {
    "typedRoutes": true,
    "reactCompiler": true
  },
  "extra": {
    "router": {},
    "eas": {
      "projectId": "7f836722-0d8e-48d8-8c53-2ebad4137d71"
    }
  },
  "owner": "kawa_ida"
}
