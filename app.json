    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1461BD",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kawa_ida.Kikwetu"
    },
    "extra": {
      "EXPO_PUBLIC_SUPABASE_URL": "https://cvnertrkcwjjdrnnjswk.supabase.co",
      "EXPO_PUBLIC_SUPABASE_ANON_KEY": "sb_publishable_4fLLnjGNmi013LGxxribpQ_dvcJvapo",
      "eas": {
        "projectId": "7f836722-0d8e-48d8-8c53-2ebad4137d71"
      }
    },
    "plugins": [
      "expo-web-browser",
      "expo-router",
      "expo-audio",
      "expo-video"
    ]
  },
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#1461BD",
        "edgeToEdgeEnabled": true
      }
